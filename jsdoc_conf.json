{
    "source": {
        "include": ["db","helpers","models", "consoleApp.js", "README.md"],
        "exclude": [ "node_modules" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "node_modules/docdash",  
        "encoding": "utf8",          
        "destination": "./docs",          
        "recurse": true
    }
}